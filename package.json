{
  "name": "einavigointiin",
  "version": "0.0.1",
  "description": "einavigointiin.fi",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "",
    "build": "npm run build:css && npm run build:assets && npm run build:js",
    "build:js": "npx esbuild --loader:.js=jsx --sourcemap --bundle --target=chrome76,firefox68,safari13 --outfile=dist/app.js src/client/app.js",
    "build:js-production": "npx esbuild --loader:.js=jsx --minify --bundle --target=chrome76,firefox68,safari13 --outfile=dist/app.js src/client/app.js",
    "build:css": "npx lessc --autoprefix='defaults' --clean-css='--s1 --advanced' assets/styles/app.less dist/styles.css",
    "build:assets": "cp -rf assets/*.svg dist/ && cp -rf assets/index.html dist/",
    "db:migrate": "source .env && db-migrate up",
    "db:reset": "source .env && db-migrate down && db-migrate up",
    "start:server": "source .env && node src/server/server.js",
    "eslint": "npx eslint src/client/",
    "eslint-fix": "npx eslint src/client/ --fix",
    "test": "npx mocha test/*",
    "connect-db": "psql einavigointiinfi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vokkim/einavigointiin.git"
  },
  "author": "Mikko Vesikkala",
  "keywords": [],
  "dependencies": {
    "@mapbox/mbtiles": "0.12.1",
    "baconjs": "3.0.17",
    "bluebird": "3.7.2",
    "browserslist": "^4.20.2",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "4.17.3",
    "lodash": "4.17.21",
    "ol": "6.14.1",
    "pg-promise": "10.11.1",
    "proj4": "2.8.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-select": "5.2.2",
    "sqlite3": "5.0.2",
    "store": "2.0.12",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "axios": "0.26.1",
    "esbuild": "^0.14.31",
    "eslint": "8.12.0",
    "eslint-plugin-react": "7.29.4",
    "less": "4.1.2",
    "mapshaper": "0.5.109",
    "mocha": "9.2.2",
    "request": "2.88.2",
    "shelljs": "0.8.5",
    "which-polygon": "2.2.0"
  }
}
